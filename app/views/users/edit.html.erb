<div>
  <div>
    <%= simple_form_for(@user) do |f| %>

      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>

      <%= f.submit "Edit your profile", class: "btn btn-success text-center" %>
    <% end %>
  </div>
</div>
